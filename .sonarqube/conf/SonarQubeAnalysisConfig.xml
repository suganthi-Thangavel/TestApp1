<?xml version="1.0"?>
<AnalysisConfig xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<SonarQubeSettings>
		<!-- Specify the SonarQube server URL -->
		<ServerUrl>http://localhost:9000</ServerUrl>
		<!-- Specify the authentication token -->
		<AuthToken>${{ secrets.SONAR_TOKEN }}</AuthToken>
	</SonarQubeSettings>
	<!-- Optionally, you can specify rules here. For now, it's left empty. -->
	<Rules />
	<!-- Optionally, you can specify analyzer settings here. For now, it's left empty. -->
	<AnalyzersSettings />
</AnalysisConfig>
